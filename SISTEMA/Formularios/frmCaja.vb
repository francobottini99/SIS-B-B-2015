    Private Sub tmrSaldos_Tick(sender As Object, e As EventArgs) Handles tmrSaldos.Tick
        Dim _Dt, _Dt2 As New DataTable
        Dim _Ver As New Mostrar
        Dim estiloEnc As New DataGridViewCellStyle()
        Dim textEnc As String = ""

        'Obtener Saldo de Caja del Día Anterior
        Dim _VerMax As New Mostrar
        Dim _DtMax As New DataTable
        Dim fecH As Date
        Dim idCj As Integer

        fecH = DateAdd(DateInterval.Day, -1, Date.Now.Date)
        _DtMax = _VerMax.verMaxIdCajaPorFecha(fecH)
        Dim _Row As DataRow = _DtMax.Rows(_DtMax.Rows.Count - 1)

        If _Row.Table.Rows.Count = 0 Then
            idCj = 0
            txtSaldoAnt.Text = Format(CDbl(0), "$ #,##0.00")
        Else
            idCj = _Row("UltimoId")
            _Dt2 = _VerMax.verSaldoCajaUltimoId(idCj)
            dgvCaja.DataSource = _Dt2
            txtSaldoAnt.Text = Format(CDbl(dgvCaja.SelectedCells.Item(10).Value), "$ #,##0.00")
        End If

        'Obtener saldo de caja del día actual
        fecH = Date.Now.Date
        _DtMax = _VerMax.verMaxIdCaja()
        Dim _Row1 As DataRow = _DtMax.Rows(_DtMax.Rows.Count - 1)
        If _Row1.Table.Rows.Count = 0 Then
            idCj = 0
            txtSaldoCaja.Text = Format(CDbl(0), "$ #,##0.00")
        Else
            idCj = _Row1("UltimoId")
            _Dt2 = _VerMax.verSaldoCajaUltimoId(idCj)
            dgvCaja.DataSource = _Dt2
            txtSaldoCaja.Text = Format(CDbl(dgvCaja.SelectedCells.Item(10).Value), "$ #,##0.00")
        End If

        SaldoDia.Text = Format(CDbl(txtSaldoCaja.Text) - CDbl(txtSaldoAnt.Text), "$ #,##0.00")

        Dim Total As Double
        Dim canV As Integer
        _Dt = _Ver.verVAloresEnVartera()
        dgvValEnCartera.DataSource = _Dt

        For Each row As DataGridViewRow In dgvValEnCartera.Rows
            Total += Val(row.Cells(7).Value)
            canV += 1
        Next

        txtImpChqCartera.Text = Format(Total, "$ #,##0.00")
        txtCantChqCartera.Text = Format(canV, "#,##0.00")
        txtEfectivo.Text = Format(CDbl(txtSaldoCaja.Text) - CDbl(txtImpChqCartera.Text), "$ #,##0.00")

        'MOSTRAR MOVIMIENTO DE CAJA DEL DIA
        _Dt2 = _Ver.verCajaDelDia(Date.Now.Date, Date.Now.Date)
        dgvCaja.DataSource = _Dt2
        dgvCaja.Columns.Item("idCaja").Visible = False
        dgvCaja.Columns.Item("CodUN").Visible = False
        dgvCaja.Columns.Item("CtaEmisorDestinatario").Visible = False
        dgvCaja.Columns.Item("Usuario").Visible = False

        estiloEnc.Font = New Font(dgvCaja.Font, FontStyle.Bold)
        dgvCaja.ColumnHeadersDefaultCellStyle = estiloEnc
        dgvCaja.ColumnHeadersDefaultCellStyle.Alignment = DataGridViewContentAlignment.MiddleCenter
        For X = 0 To 12
            If X = 8 Or X = 9 Or X = 10 Then
                dgvCaja.Columns(X).DefaultCellStyle.Alignment = DataGridViewContentAlignment.MiddleRight
                dgvCaja.Columns(X).DefaultCellStyle.Format = "$ #,##0.00"
            ElseIf X = 3 Or X = 4 Then
                dgvCaja.Columns(X).DefaultCellStyle.Alignment = DataGridViewContentAlignment.MiddleRight
            Else
                dgvCaja.Columns(X).DefaultCellStyle.Alignment = DataGridViewContentAlignment.MiddleLeft
            End If
            Select Case X
                Case 0
                    textEnc = "ID"
                    dgvCaja.Columns(0).Width = 40
                Case 1
                    textEnc = "COD UN"
                    dgvCaja.Columns(1).Width = 100
                Case 2
                    textEnc = "FECHA"
                    dgvCaja.Columns(2).Width = 40
                Case 3
                    textEnc = "HORA"
                    dgvCaja.Columns(3).Width = 80
                Case 4
                    textEnc = "TIPO MOV"
                    dgvCaja.Columns(4).Width = 80
                Case 5
                    textEnc = "CTA EMIS / DEST"
                    dgvCaja.Columns(5).Width = 100
                Case 6
                    textEnc = "EMIS / DEST"
                    dgvCaja.Columns(5).Width = 100
                Case 7
                    textEnc = "DETALLE"
                    dgvCaja.Columns(5).Width = 100
                Case 8
                    textEnc = "INGRESO"
                    dgvCaja.Columns(5).Width = 100
                Case 9
                    textEnc = "EGRESO"
                    dgvCaja.Columns(5).Width = 100
                Case 10
                    textEnc = "SALDO"
                    dgvCaja.Columns(5).Width = 100
                Case 11
                    textEnc = "OBSERVACIONES"
                    dgvCaja.Columns(5).Width = 100
                Case 12
                    textEnc = "USUARIO"
                    dgvCaja.Columns(5).Width = 100
