                                                Format(Date.Now.Date, "yyyy/MM/dd"), "-", "-", _
                                                "-", CDbl(txtImpValor.Text), "Si", txtObsValor.Text, -99)
                            End If
                            LimpiarValor()
                            ActualizaTotal()
                            lblFunValor.Text = "Nuevo"
                            txtValNum.Select()
