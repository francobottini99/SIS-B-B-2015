                                            btnGuardar.Enabled = False
                                            btnCancelar.Enabled = False
                                            txtIdCli.Enabled = True
                                            txtCliente.Enabled = True
                                            txtIdProv.Enabled = True
                                            txtProveedor.Enabled = True
